{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Get dashboard structural complexity",
			"skipFiles": ["<node_internals>/**"],
			"runtimeExecutable": "ts-node",
			"runtimeArgs": ["-T"],
			"cwd": "${workspaceFolder}/metrics/collectors",
			"program": "${workspaceFolder}/metrics/collectors/dashboard/structural-complexity.ts",
			"outFiles": ["${workspaceFolder}/**/*.js"]
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Get dashboard render times",
			"skipFiles": ["<node_internals>/**"],
			"runtimeExecutable": "node",
			"runtimeArgs": [
				"--no-deprecation",
				"--enable-source-maps",
				"--trace-warnings",
				"-r",
				"ts-node/register/transpile-only"
			],
			"cwd": "${workspaceFolder}/metrics/collectors",
			"program": "${workspaceFolder}/metrics/collectors/dashboard/render-time.ts",
			"outFiles": ["${workspaceFolder}/**/*.js"]
		}
	]
}
