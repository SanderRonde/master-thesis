	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Get dashboard structural complexity",
			"skipFiles": ["<node_internals>/**"],
			"runtimeExecutable": "ts-node",
			"runtimeArgs": ["-T"],
			"cwd": "${workspaceFolder}/metrics/collectors",
			"program": "${workspaceFolder}/metrics/collectors/dashboard/structural-complexity.ts",
			"outFiles": ["${workspaceFolder}/**/*.js"]
		}
	]
}
