{
	"compileOnSave": false,
	"extends": "./node_modules/gts/tsconfig-google.json",
	"compilerOptions": {
		"downlevelIteration": true,
		"sourceMap": true,
		"inlineSources": true,
		"declaration": false,
		"module": "CommonJS",
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"target": "ES6",
		"strict": true,
		"typeRoots": ["node_modules/@types", "types"],
		"lib": ["es2018", "dom", "dom.iterable", "esnext.array", "ESNext"],
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"noEmit": false,
		"skipLibCheck": true
	},
	"exclude": ["./submodules"],
	"include": ["scripts", "collectors", "./types"],
	"ts-node": {
		"files": true
	}
}
